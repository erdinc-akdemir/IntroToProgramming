public class Main {

    public static void main(String[] args) {

        double[] myList={9.7,1.4,4.4,9.9};
        double sum=0;
        double max=myList[0];

        for(double number:myList){
            sum+=number;
            if(number>max)
                max=number;
        }

        System.out.println("Sum = "+sum);
        System.out.println("Max = "+max);
       /* for(int i=0;i< myList.length;i++){
            System.out.println(myList[i]);
        }*/

    }
}
