public class Main {

    public static void main(String[] args) {

        // 3 SAYIDAN EN BÜYÜĞÜNÜ BULAN PROGRAM

        int sayi1=33;
        int sayi2=33;
        int sayi3=36;
        int enBuyuk=sayi1;

        if(sayi2>enBuyuk){
            enBuyuk=sayi2;
        }
        if(sayi3>enBuyuk){
            enBuyuk=sayi3;
        }

        System.out.println("En büyük sayı = "+enBuyuk);

    }
}
